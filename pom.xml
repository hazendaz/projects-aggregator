<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <groupId>org.mybatis</groupId>
  <artifactId>mybatis-aggregator</artifactId>
  <version>1.0.0-SNAPSHOT</version>
  <packaging>pom</packaging>

  <name>mybatis-aggregator</name>
  <description>The MyBatis aggregator POM.</description>
  <url>http://www.mybatis.org/</url>

  <scm>
    <url>http://github.com/mybatis/mybatis-aggregator/</url>
    <connection>scm:git:ssh://git@github.com/mybatis/mybatis-aggregator.git</connection>
    <developerConnection>scm:git:ssh://git@github.com/mybatis/mybatis-aggregator.git</developerConnection>
    <tag>HEAD</tag>
  </scm>
  <issueManagement>
    <system>GitHub Issue Management</system>
    <url>https://github.com/mybatis/mybatis-aggregator/issues</url>
  </issueManagement>
  <ciManagement>
    <system>GitHub Actions</system>
    <url>https://github.com/mybatis/mybatis-aggregator/actions</url>
  </ciManagement>
  <distributionManagement>
    <site>
      <id>gh-pages-scm</id>
      <name>Mybatis Aggregator GitHub Pages</name>
      <url>scm:git:ssh://git@github.com/mybatis/mybatis-aggregator.git</url>
    </site>
  </distributionManagement>

  <!-- Sure would be nice if git project names matched maven pom names -->
  <modules>
    <module>../base-bundle-descriptor</module>
    <module>../caffeine-cache</module>
    <module>../cdi</module>
    <!-- skip commiters-stuff -->
    <module>../ehcache-cache</module>
    <module>../freemarker-scripting</module>
    <module>../generator/core</module>
    <!-- Turn off generator eclipse for now as it has module issues used this way
    <module>../generator/eclipse</module>
    -->
    <module>../guice</module>
    <module>../hazelcast-cache</module>
    <module>../ibatis-2</module>
    <module>../ibatis2mybatis</module>
    <module>../ibatis-spring</module>
    <module>../ignite-cache</module>
    <module>../jpetstore-6</module>
    <!-- skip logo -->
    <module>../memcached-cache</module>
    <module>../migrations</module>
    <module>../migrations-maven-plugin</module>
    <!-- skip mybatipse -->
    <!-- skip mybatis.github.io -->
    <module>../mybatis-3</module>
    <module>../mybatis-bom</module>
    <module>../mybatis-dynamic-sql</module>
    <module>../mybatis-spring-boot</module>
    <module>../parent</module>
    <!-- skip self (aggregator) -->
    <module>../redis-cache</module>
    <module>../scala</module>
    <module>../spring</module>
    <module>../spring-native</module>
    <module>../thymeleaf-scripting</module>
    <module>../typehandlers-threeten-extra</module>
    <module>../velocity-scripting</module>
  </modules>

</project>
